FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "../myVars" //Include global variables

application     interFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         $t_final;

deltaT          $dt0;

writeControl    adjustableRunTime;

writeInterval   80;

purgeWrite      0;

writeFormat     binary;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   8;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           $CFLmax;

maxAlphaCo      $CFLmax;

maxDeltaT       $dtmax;

functions
{
    #includeIfPresent "../waveGaugesNProbes/surfaceElevation_controlDict"; //Needed to get surfaceElevation during runtime
}


