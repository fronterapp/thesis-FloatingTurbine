#include "../myVars" //Include global variables


turbine
{
    type            axialFlowTurbineALSource;
    active          on;

    axialFlowTurbineALSourceCoeffs
    {
        fieldNames          (U);
        selectionMode       cellSet; // cellSet || points || cellZone
        cellSet             turbine;
        origin              (0 0 $hub_h);
        axis                (-1 0 0);
        verticalDirection   (0 0 1);
        freeStreamVelocity  ($Ux 0 0);
        tipSpeedRatio       $TSR;
        rotorRadius         $R;
        velocitySampleRadius #calc "$D/10";
        nVelocitySamples    20;

        prescribedMotion
        {
            active                  on;
            rotationCenter          (0 0 $hub_h); //m
            initialRotation         (0 0 0); // deg
            motionType              harmonic1st;
            harmonic1stCoeffs
            {
                translationAmplitude    ($surge_a 0 0); // m
                translationFrequency    ($surge_f 0 0); // Hz
                rotationAmplitude       (0 0 0); // deg
                rotationFrequency       (0 0 0); // Hz
            }
        }

        dynamicStall
        {
            active          off;
            dynamicStallModel LeishmanBeddoes; // LeishmanBeddoes|LeishmanBeddoes3G|LeishmanBeddoesSD|LeishmanBeddoesSGC
        }

        endEffects
        {
            active          on;
            endEffectsModel Glauert; // Glauert || Shen || liftingLine
            GlauertCoeffs
            {
                tipEffects  on;
                rootEffects on;
            }
            /*
            ShenCoeffs
            {
                c1          0.125;
                c2          21;
                tipEffects  on;
                rootEffects on;
            }*/
        }

        blades
        {
            blade1
            {
                writePerf   true;
                writeElementPerf true;
                nElements   38;
                elementProfiles
                (
                    station1
